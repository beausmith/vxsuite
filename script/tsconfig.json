{
  "extends": "../tsconfig.shared.json",
  "compilerOptions": {
    "target": "ES2018",
    "esModuleInterop": true,
    "downlevelIteration": true,
    "lib": ["ESNext"],
    "jsx": "react-jsx",
    "module": "commonjs",
    "noEmit": true,
    "strict": true
  },
  "include": ["src"],
  "ts-node": {
    "transpileOnly": true,
    "transpiler": "ts-node/transpilers/swc-experimental"
  }
}
